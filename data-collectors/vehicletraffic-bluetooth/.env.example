# Build, only needed to test infrastructure/docker-compose.build.yml
COMPOSE_PROJECT_NAME=vehicletraffic-bluetooth
DOCKER_IMAGE=vehicletraffic-bluetooth-image
DOCKER_TAG=test-1
ARTIFACT_NAME=dc-vehicletraffic-bluetooth

# Logging level and style (text or json)
ROOT_LOG_LEVEL=info
LOG_LEVEL=debug
LOG_STYLE=text
